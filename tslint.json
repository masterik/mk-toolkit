{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": false,
    "no-console": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  }
}